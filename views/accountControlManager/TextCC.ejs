<!DOCTYPE html>
<html>
<head>
	<title>Inbox</title>
	<link rel="stylesheet" href="/assets/accountControlManager/css/acStyle.css">
	<script type="text/javascript" src="/assets/accountControlManager/js/jquery-3.5.1.js"></script>
</head>
<body>
	<%- include('acNavBar') %>
	<div id="workspace">
		<h2 align='center'>Text From Content Control Manager</h2>
		<br/>
		<table id="view">
			<tr>
				<td>CC Id</td>
				<td>Action Type</td>
				<td>Text</td>
				<td>Action</td>
			</tr>
			<% for(var i=0; i< cctext.length; i++ ){ %>
			<tr>
				<td><%= cctext[i].ccid %></td>
				<td><%= cctext[i].actiontype %></td>
				<td><%= cctext[i].text %></td>
				<td> 
					<a href="#">Reply</a>
				</td>
			</tr>
			<% } %>
		</table>
		<br/>
		<a href="/acText/Text">
			<button type="button">
				Back
			</button>
		</a>
		<br/>
		<div id="searchresult">
		</div>
	</div>
</body>
</html>