<!DOCTYPE html>
<html>
<head>
	<title>Notices</title>
	<link rel="stylesheet" href="/assets/accountControlManager/css/acStyle.css">
	<script type="text/javascript" src="/assets/accountControlManager/js/jquery-3.5.1.js"></script>
</head>
<body>
	<%- include('acNavBar') %>
	<div id="workspace">
		<h1 align='center'>Notices From Admin</h1>
		<br/>
		<table id="view">
			<tr>
				<td>Admin ID</td>
				<td>Subject</td>
				<td>Body</td>
			</tr>
			<% for(var i=0; i< notices.length; i++ ){ %>
			<tr>
				<td><%= notices[i].adminid %></td>
				<td><%= notices[i].subject %></td>
				<td><%= notices[i].body %></td>
			</tr>
			<% } %>
		</table>

		<div id="searchresult">
		</div>
	</div>
</body>
</html>