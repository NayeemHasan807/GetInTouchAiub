<%- include('menu.ejs') %>
    <div class="container-fluid">
        <main class="tm-main">
            <!-- Search form -->
            <div class="row tm-row">
                <div class="col-12">
                    <form method="GET" class="form-inline tm-mb-80 tm-search-form">                
                        <input class="form-control tm-search-input"id ="acsrct" name="query" type="text" placeholder="Search..." aria-label="Search">
                                                       
                    </form>
                </div> 
                <div id ="aclist">
                    <table border="1">
                        <tr>
                            <th>picture</th>
                           
                            <th>Acc Id</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Gender</th>
                            <th>DoB</th>
                            <th>Address</th>
                            <th>Action</th>
                        </tr>
                        <% for(var i=0; i< userlist.length; i++ ){ %>
                        <tr>
                            <td><%= userlist[i].profilepicture %></td>
                                
                                <td><%= userlist[i].acid %></td>
                                <td><%= userlist[i].name %></td>
                                <td><%= userlist[i].email %></td>
                                <td><%= userlist[i].gender %></td>
                                <td><%= userlist[i].dob %></td>
                                <td><%= userlist[i].address %></td>
                               
                                <td>
                                    <a href="/Adminhome/Notification/<%=userlist[i].acid%>">Notification</a> |
                                    <a href="/Adminhome/BlockAccCont/<%=userlist[i].acid%>">Block</a> | 
                                    <a href="/Adminhome/deleteAccCont/<%=userlist[i].acid%>">Delete</a>  
                                </td>
                            
                            <% } %>
                        </tr>
                    </table>
                </div>               
            </div>            
            <div class="row tm-row">
             
                             
            </div>            
            <footer class="row tm-row">
                <hr class="col-12">
                
                
            </footer>
        </main>
    </div>
    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/search.js"></script>
</body>
</html>