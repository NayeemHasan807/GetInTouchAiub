<%- include('menu.ejs') %>
    <div class="container-fluid">
        <main class="tm-main">
            <!-- Search form -->
            <div class="row tm-row">
                <div class="col-12">
                    <form method="GET" class="form-inline tm-mb-80 tm-search-form">                
                        <input class="form-control tm-search-input"id ="usrsrct" name="query" type="text" placeholder="Search..." aria-label="Search">
                       
                                                                                           
                    </form>
                </div>                
            </div>    
              <div class="tm-site-header ">          
                <h1  style ="color: skyblue;"class="text-center">General User  List</h1>
            </div>          
            <div class="row tm-row">
                <div id ="userchtab">
                    <table border="1">
                        <tr>
                            <th>picture</th>
                            
                            <th>Acc Id</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Gender</th>
                            <th>DoB</th>
                            <th>Address</th>
                            <th>User Type</th>
                            <th>Action</th>
                        </tr>
                        <% for(var i=0; i< userlist.length; i++ ){ %>
                        <tr>
                            <td><%= userlist[i].profilepicture %></td>
                               
                                <td><%= userlist[i].guid %></td>
                                <td><%= userlist[i].name %></td>
                                <td><%= userlist[i].email %></td>
                                <td><%= userlist[i].gender %></td>
                                <td><%= userlist[i].dob %></td>
                                <td><%= userlist[i].address %></td>
                                <td><%= userlist[i].userstatus %></td>
                               
                                <td>
                                    <a href="/Adminhome/NotificationAd/<%=userlist[i].guid%>">Notification</a> |
                                    <a href="/Adminhome/Blockuser/<%=userlist[i].guid%>">Block</a> | 
                                    <a href="/Adminhome/deleteuser/<%=userlist[i].guid%>">Delete</a>  
                                </td>
                            
                            <% } %>
                        </tr>
                    </table>
                </div>   
                             
            </div>            
            <footer class="row tm-row">
                <hr class="col-12">
                
                
            </footer>
        </main>
    </div>
     <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/search.js"></script>
</body>
</html>