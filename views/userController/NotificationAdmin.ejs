<!DOCTYPE html>
<html>
<head>
	<title>Admin Notification</title>
	<link rel="stylesheet" href="/assets/generalUser/css/guStyle.css">
	<script type="text/javascript" src="/assets/generalUser/js/jquery-3.5.1.js"></script>
</head>
<body>
	<%- include('guMenu') %>
	<div id="workspace">
		<h2 align='center'>Notification From Admin</h2>
		<br/>
		<table id="view">
			<tr>
				<td>Admin ID</td>
				<td>Subject</td>
				<td>Body</td>
			</tr>
			<% for(var i=0; i< notification.length; i++ ){ %>
			<tr>
				<td><%= notification[i].adminid %></td>
				<td><%= notification[i].subject %></td>
				<td><%= notification[i].body %></td>
			</tr>
			<% } %>
		</table>
		<br/>
		<a href="/userController/Notifications">
			<button type="button">
				Back
			</button>
		</a>
	</div>
</body>
</html>